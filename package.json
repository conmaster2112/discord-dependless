{
  "name": "discord-dependless",
  "version": "10.0.0-alpha.8",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports":{
    "./rest": {
      "default": "./dist/rest.js",
      "types":"./dist/rest.d.ts"
    },
    ".": {
      "default":"./dist/main.js",
      "types":"./dist/main.d.ts"
    }
  },
  "type": "module",
  "description": "A lightweight Discord API wrapper designed for simplicity and zero active dependencies. Unlike traditional libraries, **discord-dependless** allows sending messages persistent connection and gateway events are optional.",
  "scripts": {
    "build": "deno --allow-all --sloppy-imports ./build/index.ts & rolldown -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conmaster2112/discord-dependless.git"
  },
  "author": "conmaster2112",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conmaster2112/discord-dependless/issues"
  },
  "homepage": "https://github.com/conmaster2112/discord-dependless#readme",
  "files": [
    "./dist/*",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "discord",
    "es",
    "discordjs",
    "lightweight",
    "lite",
    "dependless",
    "es",
    "esmodule"
  ],
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "devDependencies": {
    "@types/node": "22.15.3",
    "discord-dependless": "link:",
    "oxlint": "^0.16.5",
    "rolldown": "1.0.0-beta.49",
    "rolldown-plugin-dts": "^0.17.5",
    "typescript": "^5.8.3"
  }
}
